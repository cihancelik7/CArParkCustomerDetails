<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cihancelik.CarParkDetails.HR.HrViewPage">

    <Button
        android:id="@+id/hrEmpAssigments"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:text="HrEmpAssigments"
        app:layout_constraintBottom_toBottomOf="@+id/hrEmployees"
        app:layout_constraintEnd_toStartOf="@+id/hrEmployees"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/hrEmployees" />

    <Button
        android:id="@+id/hrEmployees"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="16dp"
        android:text="HrEmployees"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hrEmpAssigments"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/hrSalaries"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="Hrsalaries"
        app:layout_constraintStart_toStartOf="@+id/hrPositions"
        app:layout_constraintTop_toBottomOf="@+id/hrPositions" />

    <Button
        android:id="@+id/hrJobs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="22dp"
        android:text="HrJobs"
        app:layout_constraintStart_toStartOf="@+id/hrLocations"
        app:layout_constraintTop_toBottomOf="@+id/hrEmpAssigments" />

    <Button
        android:id="@+id/hrLocations"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginTop="22dp"
        android:text="HrLocations"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hrJobs" />

    <Button
        android:id="@+id/hrPositions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:text="HrPositions"
        app:layout_constraintEnd_toEndOf="@+id/hrEmployees"
        app:layout_constraintTop_toBottomOf="@+id/hrEmployees" />

    <Button
        android:id="@+id/hrOrganizations"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        android:text="HrOrganization"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hrLocations" />

</androidx.constraintlayout.widget.ConstraintLayout>